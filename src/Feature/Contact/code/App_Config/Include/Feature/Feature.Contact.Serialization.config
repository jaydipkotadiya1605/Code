<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <unicorn>
            <configurations>
                <configuration name="Feature.Contact" description="Feature Contact" dependencies="Foundation.*" extends="Helix.Feature">
                    <rolePredicate>
                        <include domain="modules" pattern="^Feature Contact .*$" />
                    </rolePredicate>
                    <predicate>
                        <include name="ContactModel" database="master" path="/sitecore/layout/Models/Feature/Contact"/>
                    </predicate>
                </configuration>
            </configurations>
        </unicorn>
    </sitecore>
</configuration>