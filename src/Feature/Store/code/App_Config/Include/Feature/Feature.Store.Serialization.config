<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <unicorn>
            <configurations>
                <configuration name="Feature.Store" description="Feature Store" dependencies="Foundation.*" extends="Helix.Feature">
                    <predicate>
                        <include name="PlaceholderSettings" database="master" path="/sitecore/layout/placeholder settings/Feature/Store" />
                    </predicate>
                    <rolePredicate>
                        <include domain="modules" pattern="^Feature Store .*$" />
                    </rolePredicate>
                </configuration>
            </configurations>
        </unicorn>
    </sitecore>
</configuration>