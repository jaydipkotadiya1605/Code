@model Sitecore.Feature.Identity.Models.CopyrightModel

@if (@Model == null)
{
    return;
}

<div class="copyright">
    <div class="logo">@Model.Logo</div>
    <div class="text">@Model.Text</div>
    @if (Model.Links != null && Model.Links.Any())
    {
        <div class="links">
            @foreach (var link in @Model.Links)
            {
                @link
            }
        </div>
    }
</div>