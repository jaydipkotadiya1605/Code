<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
    <sitecore>
        <unicorn>
            <configurations>
                <configuration name="Feature.Sitemap" description="Feature Sitemap" dependencies="Foundation.*" extends="Helix.Foundation">
                    <rolePredicate>
                        <include domain="modules" pattern="^Feature Sitemap .*$" />
                    </rolePredicate>
                </configuration>
            </configurations>
        </unicorn>
    </sitecore>
</configuration>